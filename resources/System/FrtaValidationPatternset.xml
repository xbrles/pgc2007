                              <Action saveDO="memo" method="CreateErrorMemo" uri="memo://ubmatrix.com/Validation/FRTA#LanguagesNotInSeparateLinkbase" />
                   </Failure>
    </Pattern>   
    
      
    
    
    
    
    
    
    
    
